{
  "manifest_version": 3,
  "name": "Get CoCoNuT!!",
  "version": "1.01",
  "description": "click and get coconut!!",
  "permissions": ["storage"],
  "action": {
    "default_popup": "html/popup.html",
    "default_title": "확장 기능 실행"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "icons": {
    "16": "images/logo16.png",
    "48": "images/logo48.png",
    "128": "images/logo128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["json/*.json"],
      "matches": ["<all_urls>"]
    }
  ]
}
